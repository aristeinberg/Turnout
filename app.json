{
  "expo": {
    "name": "Drive Turnout",
    "slug": "Turnout",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/turnout.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18488f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.driveturnout.Turnout",
      "buildNumber": "2",
      "infoPlist": {
        "NSContactsUsageDescription": "This app uses your address book to help you find people to encourage to vote."
      }
    },
    "android": {
      "package": "com.driveturnout.Turnout",
      "versionCode": 1,
      "permissions": [
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
